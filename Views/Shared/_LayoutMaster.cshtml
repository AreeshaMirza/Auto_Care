<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    

    <link href="~/Content/custom.css" rel="stylesheet" />
    
    <link href="~/links/scrolling-nav.css" rel="stylesheet" />
    <link href="~/links/animate.min.css" rel="stylesheet" />
    <link href="~/links/font-awesome.min.css" rel="stylesheet" />
    <link href="~/links/w3.css" rel="stylesheet" />
</head>
<body id="page-top" data-spy="scroll" data-target=".navbar-fixed-top">

    
        <nav class="navbar navbar-inverse navbar-fixed-top " role="navigation" style="background-color: rgba(64, 54, 54, 0.78); ">
            <div class="container">
                <div class="navbar-header page-scroll">
                    <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".js-navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand page-scroll" href="#page-top" onmouseover="this.style.color = ' #fff'" onmouseout="    this.style.color = '#12A8F1'"
                       style=" margin-top: 12px; font-family: gabriola; font-size: 80px; color: #12A8F1;"><strong>Auto<i style="font-size: 40px; color:#cfcbcb;" class="fa">&#xf1b9;</i>Care</strong></a>
                </div>
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse js-navbar-collapse pull-right">
                    <ul class="nav navbar-nav">
                        <!-- Hidden li included to remove active class from about link when scrolled up past about section -->
                        <li class="hidden active">
                            <a class="page-scroll" href="#page-top"></a>
                        </li>
                        <li>
                            <a class="page-scroll link-3" id="nav-style" href="#1" onmouseover="this.style.color = ' #fff'" onmouseout="    this.style.color = '#12A8F1'">HOME</a>
                        </li>
                        <li>
                            <a class="page-scroll link-3" id="nav-style" href="#2" onmouseover="this.style.color = ' #fff'" onmouseout="    this.style.color = '#12A8F1'">ABOUT</a>
                        </li>
                        <li>
                            <a class="page-scroll link-3" id="nav-style" href="#3" onmouseover="this.style.color = ' #fff'" onmouseout="    this.style.color = '#12A8F1'">LOGIN</a>
                        </li>
                        <li>
                            <a class="page-scroll link-3" id="nav-style" href="#4" onmouseover="this.style.color = ' #fff'" onmouseout="    this.style.color = '#12A8F1'">SIGNUP</a>
                        </li>
                        <li>
                            <a class="page-scroll link-3" id="nav-style" href="#5" onmouseover="this.style.color = ' #fff'" onmouseout="    this.style.color = '#12A8F1'">SERVICES</a>
                        </li>
                        <li>
                            <a class="page-scroll link-3" id="nav-style" href="#6" onmouseover="this.style.color = ' #fff'" onmouseout="    this.style.color = '#12A8F1'">FEEDBACK</a>
                        </li>
                        <li>
                            <a class="page-scroll link-3" id="nav-style" href="#7" onmouseover="this.style.color = ' #fff'" onmouseout="    this.style.color = '#12A8F1'">CONTACT US</a>
                        </li>

                    </ul>
                </div>
            </div>
</nav>
        <!-- /.navbar-collapse -->
    
    <!-- /.container -->

    <div>
        @RenderBody()
    </div>

    <script src="~/Scripts/bootstrap.min.js"></script>

    <script src="~/Scripts/jquery-1.9.1.js"></script>
    <script src="~/links/jquery.scrollNav.js"></script>
    <script src="~/links/jquery.js"></script>
    <script src="~/links/jquery.scrollNav.min.js"></script>
    <link href="~/links/animate.css" rel="stylesheet" />
    <script src="~/links/scrolling-nav.js"></script>

</body>
</html>