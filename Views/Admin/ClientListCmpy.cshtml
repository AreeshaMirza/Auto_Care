        <h1 style="color:black; font-weight: 700; font-family: Gabriola; text-align: left; font-size: 50px; margin-left: 25px;">List>Company</h1>
        <div class="row">
            <div class="col-sm-4" id="form-style">
                <input type="number" class="form-control" placeholder="Organization Number" required style="margin-left: 12%; width: 90%; height: 45px; border-width: 2px; border-color: #12A8F1;">
            </div>
            <div class="col-sm-4" id="form-style">
                <input type="text" class="form-control" placeholder="Company Name" required style="margin-left: 12%; width: 90%; height: 45px; border-width: 2px; border-color: #12A8F1;">
            </div>
            <div class="col-sm-4" id="form-style">
                <input type="text" class="form-control" placeholder="Contact Person" required style="margin-left: 12%; width: 90%; height: 45px; border-width: 2px; border-color: #12A8F1;">
            </div>
        </div>

        <div class="row">
            <div class="col-sm-6" id="form-style">
                <input type="number" class="form-control" placeholder="CP Phone Number" required style="margin-left: 12%; width: 90%; height: 45px; border-width: 2px; border-color: #12A8F1;">
            </div>
            <div class="col-sm-6" id="form-style">
                <input type="email" class="form-control" placeholder="CP Email" required style="margin-left: 12%; width: 90%; height: 45px; border-width: 2px; border-color: #12A8F1;">
            </div>
        </div>

        <button type="submit" id="fbutton">Search</button>
    </form>
</div>


